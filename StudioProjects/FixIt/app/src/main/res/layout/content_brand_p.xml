<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.fixit.fixit.brand_p"
    tools:showIn="@layout/activity_brand_p"
    android:background="@color/nice_plain"
    android:orientation="vertical">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_android"
        android:layout_width="300dp"
        android:layout_height="250dp"
        android:onClick="ch_android"
        app:backgroundTint="@color/fab"
        app:srcCompat="@drawable/android"
        android:layout_gravity="center"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab_apple"
        android:layout_width="300dp"
        android:layout_height="250dp"
        android:layout_gravity="center"
        android:onClick="ch_apple"
        app:backgroundTint="@color/fab"
        app:srcCompat="@drawable/appple" />

</LinearLayout>
